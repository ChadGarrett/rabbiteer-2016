{
  "name": "rabbiteer-2016",
  "version": "0.0.1",
  "description": "A skeleton for a NodeJS application",
  "keywords": [
    "node project",
    "nodejs project",
    "node seed",
    "eslint",
    "mocha",
    "istanbul",
    "code coverage",
    "gulp",
    "pre-commit",
    "es6",
    "babel"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./build/coverage"
  },
  "main": "",
  "scripts": {
    "pre-commit": "npm run build",
    "compile": "babel src --out-dir build/source --source-maps",
    "start": "npm run compile && node build/source",
    "run": "babel-node src",
    "test": "mocha",
    "coverage": "nyc mocha",
    "lint": "eslint src test www/app",
    "build": "gulp",
    "docs": "jsdoc -d build/jsdoc src",
    "postinstall": "bower install",
    "gulp": "gulp"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RetroRabbit/rabbiteer-2016"
  },
  "author": "Jade Abbott",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RetroRabbit/rabbiteer-2016"
  },
  "homepage": "https://github.com/RetroRabbit/rabbiteer-2016",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "bower": "^1.7.9",
    "chai": "^3.5.0",
    "eslint": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-help": "^1.6.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-jsdoc3": "^1.0.0",
    "gulp-mocha": "^3.0.1",
    "gulp-webpack": "^1.5.0",
    "isparta": "^4.0.0",
    "jsdoc": "^3.4.1",
    "mocha": "^3.0.2",
    "nyc": "^8.3.0",
    "opn": "^4.0.2",
    "pre-commit": "^1.1.3",
    "run-sequence": "^1.2.2",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "morgan": "^1.7.0"
  }
}
